        <action
            android:id="@+id/action_q7_FormFragment_to_q8_FormFragment"
            app:destination="@id/q8_FormFragment" />
    </fragment>
    <fragment
        android:id="@+id/q8_FormFragment"
        android:name="com.example.investoranalysisapplication.questionnaire.Q8_FormFragment"
        android:label="fragment_q8__form"
        tools:layout="@layout/fragment_q8__form" >
        <action
            android:id="@+id/action_q8_FormFragment_to_q9_FormFragment"
            app:destination="@id/q9_FormFragment" />
    </fragment>
    <fragment
        android:id="@+id/q9_FormFragment"
        android:name="com.example.investoranalysisapplication.questionnaire.Q9FormFragment"
        android:label="fragment_q9__form"
        tools:layout="@layout/fragment_q9__form" >
        <action
            android:id="@+id/action_q9_FormFragment_to_resultActivity"
            app:destination="@id/resultActivity" />
    </fragment>
    <activity
        android:id="@+id/resultActivity"
        android:name="com.example.investoranalysisapplication.ResultActivity"
        android:label="activity_result"
        tools:layout="@layout/activity_result" />
</navigation>