        <action
            android:id="@+id/action_q4_FormFragment_to_q5_FormFragment"
            app:destination="@id/q5_FormFragment" />
    </fragment>
    <fragment
        android:id="@+id/q5_FormFragment"
        android:name="com.example.investoranalysisapplication.questionnaire.Q5_FormFragment"
        android:label="fragment_q5__form"
        tools:layout="@layout/fragment_q5__form" >
        <action
            android:id="@+id/action_q5_FormFragment_to_q6_FormFragment"
            app:destination="@id/q6_FormFragment" />
    </fragment>
    <fragment
        android:id="@+id/q6_FormFragment"
        android:name="com.example.investoranalysisapplication.questionnaire.Q6_FormFragment"
        android:label="fragment_q6__form"
        tools:layout="@layout/fragment_q6__form" >
        <action
            android:id="@+id/action_q6_FormFragment_to_q7_FormFragment"
            app:destination="@id/q7_FormFragment" />
    </fragment>
    <fragment
        android:id="@+id/q7_FormFragment"
        android:name="com.example.investoranalysisapplication.questionnaire.Q7_FormFragment"
