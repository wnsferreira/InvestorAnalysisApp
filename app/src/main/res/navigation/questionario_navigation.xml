<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/questionario_navigation"
    app:startDestination="@id/q1_Fragment">

    <fragment
        android:id="@+id/q1_Fragment"
        android:name="com.example.investoranalysisapplication.questionnaire.Q1_Fragment"
        android:label="fragment_q1_form"
        tools:layout="@layout/fragment_q1_form" >
        <action
            android:id="@+id/action_q1_Fragment_to_q2_FormFragment"
            app:destination="@id/q2_FormFragment" />
    </fragment>
    <fragment
        android:id="@+id/q2_FormFragment"
        android:name="com.example.investoranalysisapplication.questionnaire.Q2_FormFragment"
        android:label="fragment_q2__form"
        tools:layout="@layout/fragment_q2__form" >
        <action
            android:id="@+id/action_q2_FormFragment_to_q3_FormFragment"
            app:destination="@id/q3_FormFragment" />
    </fragment>
    <fragment
        android:id="@+id/q3_FormFragment"
        android:name="com.example.investoranalysisapplication.questionnaire.Q3_FormFragment"
        android:label="fragment_q3__form"
